version: '2.1'
services:
  haproxy_password:
    image: "haproxy:${version:-1.8}-alpine"
    ports:
      - 3835
    labels:
        com.datadoghq.sd.check.id: "haproxy"
    volumes:
      - ./haproxy-password.cfg:/usr/local/etc/haproxy/haproxy.cfg

networks:
  default:
    external:
      name: workbench
